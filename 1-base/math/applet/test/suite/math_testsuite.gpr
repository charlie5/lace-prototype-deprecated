with "ahven",
     "math";


project Math_Testsuite is

   for Object_Dir use "build";
   for Exec_Dir use ".";
   for Main use ("launch_math_testsuite.adb");

   package Builder is
      for Default_Switches ("ada") use ("-g");
   end Builder;

   package Compiler is
      for Default_Switches ("ada") use ("-gnat12");
   end Compiler;

   package Binder is
      for Default_Switches ("ada") use ("-E");
   end Binder;

   package Linker is
      for Default_Switches ("ada") use ("-g");
   end Linker;

end Math_Testsuite;

